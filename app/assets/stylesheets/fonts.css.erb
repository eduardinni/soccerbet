@font-face {
    font-family: 'lcdmono2normal';
    src: url('<%= asset_path('lcdm2n__-webfont.eot')%>');
    src: url('<%= asset_path('lcdm2n__-webfont.eot')%>?#iefix') format('embedded-opentype'),
         url('<%= asset_path('lcdm2n__-webfont.woff')%>') format('woff'),
         url('<%= asset_path('lcdm2n__-webfont.ttf')%>') format('truetype'),
         url('<%= asset_path('lcdm2n__-webfont.svg')%>#lcdmono2normal') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'brazil_2014regular';
    src: url('<%= asset_path('brasil_2014-webfont.eot')%>');
    src: url('<%= asset_path('brasil_2014-webfont.eot')%>?#iefix') format('embedded-opentype'),
         url('<%= asset_path('brasil_2014-webfont.woff')%>') format('woff'),
         url('<%= asset_path('brasil_2014-webfont.ttf')%>') format('truetype'),
         url('<%= asset_path('brasil_2014-webfont.svg')%>#brazil_2014regular') format('svg');
    font-weight: normal;
    font-style: normal;

}
